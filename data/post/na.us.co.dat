ceiving and interpretin"},"formattedEmploymentStatus":"Full-time","title":"Clinical Data Analyst I - 56014 - CO6908533","entityUrn":"urn:li:fs_normalized_jobPosting:315442717","jobRegion":"urn:li:fs_region:(us,34)","applyMethod":{"com.linkedin.voyager.jobs.OffsiteApply":{"applyStartersPreferenceVoid":false,"companyApplyUrl":"https://my.jobs/59EAFF0BC1424F31B4E805A51F07BD715044"}},"closedAt":1492205062000,"views":0,"savingInfo":{"saved":false,"entityUrn":"urn:li:fs_jobSavingInfo:315442717"},"new":false,"eligibleForReferrals":false,"formattedJobFunctions":["Information Technology"],"formattedLocation":"Aurora, CO, US","listedAt":1491347237000,"expireAt":1494524261000,"jobPostingUrl":"https://www.linkedin.com/jobs/view/315442717/?trk=jobs_biz_prem_srch","formattedExperienceLevel":"Associate","applies":0,"formattedIndustries":["Nonprofit Organization Management","Health, Wellness and Fitness","Hospital &amp; Health Care"],"entityUrnResolutionResult":{"entityUrn":"urn:li:fs_normalized_jobPosting:315442717","relevanceReasonInjectionResult":{"entityUrn":"urn:li:fs_jobPostingRelevanceReason:(urn:li:fs_normalized_jobPosting:315442717,_)","jobPosting":"urn:li:fs_normalized_jobPosting:315442717","details":{"com.linkedin.voyager.jobs.shared.HiddenGemRelevanceReasonDetails":{}}}},"sourceDomain":"my.jobs","applyingInfo":{"entityUrn":"urn:li:fs_jobApplyingInfo:315442717","applied":false},"jobRegionResolutionResult":{"entityUrn":"urn:li:fs_region:(us,34)","regionName":"Greater Denver Area"}}\n  '
