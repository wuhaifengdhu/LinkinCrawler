cancy."},"listedAt":1488931200000,"expireAt":1494522553000,"jobPostingUrl":"https://www.linkedin.com/jobs/view/296852862/?trk=jobs_biz_prem_srch","formattedExperienceLevel":"Entry level","applies":110,"formattedIndustries":["Information Technology and Services","Computer Software","Staffing and Recruiting"],"entityUrnResolutionResult":{"entityUrn":"urn:li:fs_normalized_jobPosting:296852862","relevanceReasonInjectionError":{"message":"Downstream service returned an empty response for URN.","status":404}},"sourceDomain":"www.nigelfrank.com","applyingInfo":{"entityUrn":"urn:li:fs_jobApplyingInfo:296852862","applied":false},"jobRegionResolutionResult":{"entityUrn":"urn:li:fs_region:(us,14)","regionName":"Greater Chicago Area"}}\n  '
